{% extends 'base/user.html.twig' %}

{% block body %}

    {% for nichoir in user.nichoirs %}
        <div class="card" style="border: solid black 1px;">
            <h1>{{ nichoir.nom }}</h1>
            <h1>{{ nichoir.photo }}</h1>
            <h1>{{ nichoir.date_installation | date("m/d/Y") }}</h1>
        </div>
    {% endfor %}

{% endblock %}
